# Generated by Chef
# Local modifications will be overwritten.
#
# Bareos Director Fileset Configuration File

Fileset {
  Name = "<%= @fileset_name %>"
<% @fileset_custom_strings.each do |string| %>
  <%= string %>
<% end unless @fileset_custom_strings.nil? %>
<% @fileset_include_config.each do |include_comment, include_components| %>
  Include {    # <%= include_comment %>
  <% include_components['options'].each do |option_comment, option_strings| %>
    Options {     # <%= option_comment %>
    <% option_strings.each do |string| %>
      <%= string %>
    <% end unless option_strings.nil? %>
    }
  <% end unless include_components['options'].nil? %>
  <% include_components['file'].each do |string| %>
    <%= string %>
  <% end unless include_components['file'].nil? %>
  <% include_components['exclude_dir_containing'].each do |string| %>
    <%= string %>
  <% end unless include_components['exclude_dir_containing'].nil? %>
  }
<% end unless @fileset_include_config.nil? %>
<% @fileset_exclude_config.each do |exclude_comment, exclude_strings| %>
  Exclude {     # <%= exclude_comment %>
  <% exclude_strings.each do |string| %>
    <%= string %>
  <% end unless exclude_strings.nil? %>
  }
<% end unless @fileset_exclude_config.nil? %>
}
